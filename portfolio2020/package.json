{
  "name": "portfolio2020",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "node-sass-chokidar": "^1.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-pdf": "^5.0.0",
    "react-scripts": "4.0.0",
    "react-stickynode": "^3.0.4",
    "react-window-size": "^1.2.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "SET PORT=3001 && react-scripts start",
    "build": "SET PORT=3001 && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/styles/sass/main.scss -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass/main.scss -o src/styles/css --watch --usePolling --polling-interval 500"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
